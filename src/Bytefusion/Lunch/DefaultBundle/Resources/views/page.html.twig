{% extends '@BytefusionLunchDefault/base.html.twig' %}
{% block body %}
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
                        aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Lunchsite</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                {{ knp_menu_render('BytefusionLunchDefaultBundle:Builder:leftNavbar', {'currentClass': 'active'}) }}
                {{ knp_menu_render('BytefusionLunchDefaultBundle:Builder:rightNavbar', {'currentClass': 'active'}) }}
            </div>
        </div>
    </nav>
    <div class="container">
        {% block content %}

        {% endblock %}
    </div><!-- /.container -->
{% endblock %}