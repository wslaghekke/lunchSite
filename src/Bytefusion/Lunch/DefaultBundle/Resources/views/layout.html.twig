{#==========================================================#}
{#            Layout template for FOSUserBundle             #}
{#==========================================================#}
{% extends '@BytefusionLunchDefault/page.html.twig' %}
{% block content %}
    <div class="row">
        <div class="col-md-12">
            <h1>Login</h1>

            {% if app.request.hasPreviousSession %}
                {% for type, messages in app.session.flashbag.all() %}
                    {% for message in messages %}
                        <div class="alert alert-{{ type }}">
                            {{ message }}
                        </div>
                    {% endfor %}
                {% endfor %}
            {% endif %}

            <div>
                {% block fos_user_content %}
                {% endblock fos_user_content %}
            </div>
        </div>
    </div>
{% endblock %}